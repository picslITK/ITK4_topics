IF(USE_FFTWF OR USE_FFTWD)
  WRAP_CLASS("itk::FFTWComplexToComplexImageFilter" POINTER)
    foreach(d ${WRAP_ITK_DIMS})
      if(USE_FFTWF AND WRAP_complex_float)
        WRAP_TEMPLATE("${ITKM_ICF}${d}" "${ITKT_ICF${d}}")
      endif()

      if(USE_FFTWD AND WRAP_complex_double)
        WRAP_TEMPLATE("${ITKM_ICD}${d}" "${ITKT_ICD${d}}")
      endif()
    endforeach(d)
  END_WRAP_CLASS()
endif()
END_WRAP_CLASS()
