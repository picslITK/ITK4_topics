if(USE_FFTWF OR USE_FFTWD)
  WRAP_CLASS("itk::FFTWRealToHalfHermitianForwardFFTImageFilter" POINTER)
    foreach(d ${WRAP_ITK_DIMS})
      if(USE_FFTWF AND WRAP_complex_float AND WRAP_float)
        WRAP_TEMPLATE("${ITKM_IF}${d}${ITKM_ICF}${d}" "${ITKT_IF${d}}, ${ITKT_ICF${d}}")
      endif()

      if(USE_FFTWD AND WRAP_complex_double AND WRAP_double)
        WRAP_TEMPLATE("${ITKM_ID}${d}${ITKM_ICD}${d}" "${ITKT_ID${d}}, ${ITKT_ICD${d}}")
      endif()
    endforeach(d)
  END_WRAP_CLASS()
endif()
